        "unit": "jest --config ./config/jest/jest.config.ts",
        "cy:e2e": "cypress open",
        "storybook": "storybook dev -p 6006 -c ./config/storybook",
        "build-storybook": "storybook build -c ./config/storybook",
        "chromatic": "npx chromatic",
        "prepare": "husky",
        "generate:slice": "node ./scripts/createSlice/index.js",
        "postinstall": "node ./scripts/clear-cache.js",
        "removeFeature": "npx ts-node scripts/remove-feature.ts enableCounter on"
    },
    "lint-staged": {
        "**/*.{ts,tsx}": [
            "npm run prettier",
            "npm run lint"
        ]
